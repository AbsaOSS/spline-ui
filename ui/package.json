        "lint-staged": "^10.1.3",
        "loader-utils": "^2.0.4",
        "lodash": "^4.17.21",
        "mockdate": "^3.0.5",
        "ng-mocks": "^14.6.0",
        "ng-packagr": "^12.1.2",
        "ngrx-store-freeze": "^0.2.4",
        "replace-in-file": "^6.1.0",
        "shx": "^0.3.3",
        "stylelint": "^13.3.1",
        "stylelint-config-sass-guidelines": "^7.0.0",
        "stylelint-scss": "^3.16.1",
        "sync-pom-version-to-package": "^1.6.1",
        "ts-jest": "^27.0.4",
