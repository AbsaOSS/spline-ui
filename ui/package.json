{
    "name": "spline-ui",
    "version": "1.0.0-SNAPSHOT",
    "author": "ABSA Group Limited Developers Team",
    "license": "Apache 2.0",
    "scripts": {
        "postinstall": "sync-pom-version",
        "ng": "ng",
        "start": "ng serve --open --port 4300",
        "start:localhost": "ng serve -c localhost --open --port 4300",
        "build": "ng build --configuration production --deploy-url=../ --base-href=SPLINE_UI_DEPLOY_CONTEXT/app/",
        "build::CDN": "ng build --configuration production",
        "postbuild": "shx mkdir dist/app/ && shx mv dist/index.html dist/app/ && shx mv dist/assets dist/app/",
        "test": "jest --passWithNoTests --coverage",
        "test:no-coverage": "jest --passWithNoTests",
        "lint": "npm run lint:ts && npm run lint:styles",
        "lint:ts": "eslint src/**/*.ts projects/**/*.ts --quiet",
        "lint:ts:fix": "eslint src/**/*.ts projects/**/*.ts --fix",
        "lint:styles": "stylelint \"src/**/*.scss\"",
        "lint:styles:fix": "stylelint src/**/*.scss --fix",
        "e2e": "ng e2e",
        "ngcc": "ngcc --properties es2015 browser module main --async false",
        "license-check": "cd ../ && mvn validate -Plicense-check",
        "pre-pull-request": "npm run lint && npm run test:no-coverage && npm run license-check"
    },
    "private": false,
    "jscpd": {
        "threshold": 0.1,
        "reporters": [
            "html",
            "console",
            "badge"
        ],
        "ignore": [
            "**/node_modules/**",
            "**/*.min.js,**/*.map",
            "**/coverage/**"
        ],
        "absolute": true,
        "gitignore": true
    },
    "dependencies": {
        "@angular/animations": "12.1.3",
        "@angular/cdk": "12.1.3",
        "@angular/common": "12.1.3",
        "@angular/compiler": "12.1.3",
        "@angular/core": "12.1.3",
        "@angular/forms": "12.1.3",
        "@angular/material": "12.1.3",
        "@angular/platform-browser": "12.1.3",
        "@angular/platform-browser-dynamic": "12.1.3",
        "@angular/router": "12.1.3",
        "@ngrx/effects": "12.3.0",
        "@ngrx/entity": "12.3.0",
        "@ngrx/router-store": "12.3.0",
        "@ngrx/store": "12.3.0",
        "@ngx-translate/core": "13.0.0",
        "@ngx-translate/http-loader": "6.0.0",
        "@swimlane/ngx-graph": "7.2.0",
        "bootstrap": "4.4.1",
        "d3-shape": "1.3.7",
        "lodash-es": "4.17.21",
        "moment": "2.29.4",
        "ngx-daterangepicker-material": "3.0.4",
        "ngx-json-viewer": "3.0.2",
        "ngx-translate-multi-http-loader": "3.0.0",
        "rxjs": "6.5.4",
        "tslib": "2.3.0",
        "zone.js": "0.11.4"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "15.0.0",
        "@angular-builders/jest": "12.1.0",
        "@angular-devkit/build-angular": "15.0.4",
        "@angular-eslint/builder": "15.1.0",
        "@angular-eslint/eslint-plugin": "0.0.1-alpha.27",
        "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.27",
        "@angular-eslint/template-parser": "0.0.1-alpha.27",
        "@angular/cli": "12.1.3",
        "@angular/compiler-cli": "12.1.3",
        "@angular/language-service": "12.1.3",
        "@babel/helper-get-function-arity": "^7.16.7",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/runtime": "^7.14.6",
        "@ngrx/store-devtools": "12.3.0",
        "@types/d3-shape": "1.3.2",
        "@types/jest": "26.0.24",
        "@types/lodash-es": "4.17.4",
        "@types/node": "12.11.1",
        "@typescript-eslint/eslint-plugin": "2.27.0",
        "@typescript-eslint/eslint-plugin-tslint": "2.27.0",
        "@typescript-eslint/parser": "2.27.0",
        "codelyzer": "6.0.0",
        "eslint": "6.8.0",
        "eslint-plugin-import": "2.21.2",
        "eslint-plugin-unused-imports": "0.1.2",
        "fs-extra": "9.0.1",
        "husky": "4.3.8",
        "jest": "27.0.6",
        "jest-preset-angular": "9.0.5",
        "jest-teamcity-reporter": "0.9.0",
        "lint-staged": "10.1.3",
        "lodash": "4.17.21",
        "ng-packagr": "12.1.2",
        "ngrx-store-freeze": "0.2.4",
        "replace-in-file": "6.1.0",
        "shx": "0.3.3",
        "stylelint": "13.3.1",
        "stylelint-config-sass-guidelines": "7.0.0",
        "stylelint-scss": "3.16.1",
        "sync-pom-version-to-package": "1.6.0",
        "ts-jest": "27.0.4",
        "ts-node": "8.3.0",
        "tslint": "6.1.0",
        "tslint-angular": "3.0.2",
        "typescript": "4.3.5"
    },
    "engines": {
        "npm": ">=7.0.0"
    }
}
