{
  "name": "spline-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 4300 --base-href=/appp/",
    "start:localhost": "ng serve -c localhost --open --port 4300 --base-href=/app/",
    "build": "ng build --base-href=/app/",
    "build:prod": "ng build --prod --base-href=/app/",
    "test": "jest --passWithNoTests",
    "lint": "ng lint && npm run lint:styles",
    "lint:ts": "eslint src/**/*.ts",
    "lint:styles": "stylelint src/**/*.scss",
    "lint:styles:fix": "stylelint src/**/*.scss --fix",
    "e2e": "ng e2e",
    "ngcc": "ngcc --properties es2015 browser module main --async false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@ngrx/effects": "^9.1.0",
    "@ngrx/router-store": "^9.1.0",
    "@ngrx/store": "^9.1.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-graph": "^7.0.1",
    "bootstrap": "^4.4.1",
    "cytoscape": "^3.14.1",
    "cytoscape-klay": "^3.1.2",
    "cytoscape-node-html-label": "~1.1.3",
    "d3-shape": "^1.3.7",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular-devkit/build-ng-packagr": "~0.901.1",
    "@angular-eslint/builder": "0.0.1-alpha.27",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.27",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.27",
    "@angular-eslint/template-parser": "0.0.1-alpha.27",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.11",
    "@angular/language-service": "~9.1.11",
    "@ngrx/store-devtools": "^9.1.0",
    "@types/cytoscape": "^3.14.5",
    "@types/d3-shape": "^1.3.2",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "codelyzer": "^5.1.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-unused-imports": "^0.1.2",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-preset-angular": "^8.1.3",
    "lint-staged": "^10.1.3",
    "ng-packagr": "^9.0.0",
    "ngrx-store-freeze": "^0.2.4",
    "protractor": "~5.4.3",
    "stylelint": "^13.3.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.16.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.2",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --syntax=scss --fix",
      "git add"
    ],
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  }
}
